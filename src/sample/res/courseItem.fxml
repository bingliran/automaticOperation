<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="40.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="sample.controller.CourseItemController">
    <Label fx:id="title" layoutY="5.0" prefHeight="30.0" prefWidth="140.0" style="-fx-alignment: center;" text="标题"
           textFill="#999595">
        <font>
            <Font size="8.0"/>
        </font>
    </Label>
    <Label fx:id="startTime" layoutX="140.0" layoutY="5.0" prefHeight="30.0" prefWidth="70.0"
           style="-fx-alignment: center;" text="开始时间" textFill="#999595">
        <font>
            <Font size="8.0"/>
        </font>
    </Label>
    <Label fx:id="endTime" layoutX="205.0" layoutY="5.0" prefHeight="30.0" prefWidth="70.0"
           style="-fx-alignment: center;" text="截止时间" textFill="#999595">
        <font>
            <Font size="8.0"/>
        </font>
    </Label>
    <Button layoutX="285.0" layoutY="12.0" mnemonicParsing="false" onAction="#seeAnswer" prefHeight="18.0"
            prefWidth="45.0" text="查看答案" textFill="#5a5757">
        <font>
            <Font size="8.0"/>
        </font>
    </Button>
    <Button layoutX="340.0" layoutY="12.0" mnemonicParsing="false" onAction="#automaticAnswer" prefHeight="18.0"
            prefWidth="45.0" text="自动答题" textFill="#5a5757">
        <font>
            <Font size="8.0"/>
        </font>
    </Button>
</Pane>
